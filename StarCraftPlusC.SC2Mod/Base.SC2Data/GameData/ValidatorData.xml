<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CValidatorUnitType id="AutoCreepTargetNode">
        <Value value="GameAutoCreepNodeTarget"/>
    </CValidatorUnitType>
    <CValidatorCombine id="AutoCreepTargetNodeReachable">
        <Type value="And"/>
        <CombineArray value="AutoCreepTargetNode"/>
        <CombineArray value="CliffLevelEqualSource"/>
        <CombineArray value="TargetOnCreep"/>
    </CValidatorCombine>
    <CValidatorUnitType id="AutoWallConstructionFootprint">
        <Value value="SupplyDepotConstructionFootprint"/>
    </CValidatorUnitType>
    <CValidatorUnitCompareBehaviorCount id="AutoWallLoweredManual">
        <Behavior value="AutoWallManualRaiseCooldown"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="AutoWallAutocastEnabled">
        <Value value="1"/>
        <Behavior value="AutoWallAutocastBehavior"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorLocationCompareCliffLevel id="CliffLevelEqualSource">
        <WhichLocation Value="TargetUnit"/>
        <OtherLocation Value="SourceUnit"/>
    </CValidatorLocationCompareCliffLevel>
    <CValidatorLocationEnumArea id="IsNexusNearby">
        <AreaArray Radius="6.5" Validator="IsNexus"/>
    </CValidatorLocationEnumArea>
    <CValidatorUnitCompareBehaviorCount id="IsNexusNearbyBehav">
        <Value value="1"/>
        <Behavior value="IsNexusNearby"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="IsNexusNotNearbyBehav">
        <Behavior value="IsNexusNearby"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="IsWarpgateNearbyBehav">
        <Value value="1"/>
        <Behavior value="IsWarpGateNearby"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorLocationEnumArea id="IsWarpGateNearby">
        <AreaArray Radius="4.75" Validator="IsWarpgate"/>
    </CValidatorLocationEnumArea>
    <CValidatorCombine id="NexusWarpGateNotNearby">
        <Type value="And"/>
        <CombineArray value="IsNexusNearbyBehav"/>
        <CombineArray value="IsWarpgateNearbyBehav"/>
        <Negate value="1"/>
    </CValidatorCombine>
    <CValidatorCombine id="NexusNotNearbyWarpGateNearby">
        <Type value="And"/>
        <CombineArray value="IsNexusNotNearbyBehav"/>
        <CombineArray value="IsWarpgateNearbyBehav"/>
    </CValidatorCombine>
    <CValidatorUnitCompareChargeUsed id="OverlordDontRemoveCooldownifNoCharges">
        <WhichUnit Value="Source"/>
        <OtherUnit Value="Source"/>
        <ChargeLink value="Abil/CreepTumorBuild"/>
        <Location value="Unit"/>
        <Ability value="CreepTumorBuild"/>
    </CValidatorUnitCompareChargeUsed>
    <CValidatorLocationEnumArea id="SupplyDepotLowerEnemyEnumerate">
        <WhichLocation Value="OriginUnit"/>
        <AreaArray Radius="2.5"/>
        <Compare value="Eq"/>
        <SearchFlags index="ExtendByUnitRadius" value="1"/>
        <LaunchLocation Value="OriginUnit"/>
        <SearchFilters value="Ground;Self,Player,Ally,Neutral,Structure,Missile,Dead,Hidden"/>
    </CValidatorLocationEnumArea>
    <CValidatorLocationEnumArea id="SupplyDepotRaiseAllyEnumerate">
        <WhichLocation Value="OriginUnit"/>
        <AreaArray Radius="2.5"/>
        <Compare value="Eq"/>
        <SearchFlags index="ExtendByUnitRadius" value="1"/>
        <LaunchLocation Value="OriginUnit"/>
        <SearchFilters value="Ground;Self,Neutral,Enemy,Structure,Missile,Hidden"/>
    </CValidatorLocationEnumArea>
    <CValidatorLocationEnumArea id="SupplyDepotRaiseEnemyEnumerate">
        <WhichLocation Value="OriginUnit"/>
        <AreaArray Radius="2.5"/>
        <SearchFlags index="ExtendByUnitRadius" value="1"/>
        <LaunchLocation Value="OriginUnit"/>
        <SearchFilters value="Ground;Self,Player,Ally,Neutral,Structure,Missile,Hidden"/>
    </CValidatorLocationEnumArea>
    <CValidatorCombine id="SupplyDepotRaiseNot">
        <Type value="And"/>
        <CombineArray value="SupplyDepotLowerEnemyEnumerate"/>
        <Negate value="1"/>
    </CValidatorCombine>
    <CValidatorCombine id="SupplyDepotRaiseOr">
        <CombineArray value="SupplyDepotRaiseEnemyEnumerate"/>
        <CombineArray value="SupplyDepotRaiseAllyEnumerate"/>
    </CValidatorCombine>
</Catalog>
