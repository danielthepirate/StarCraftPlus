        lv_tickTotalVespene *= 0.75;
    }
    if ((lv_progressIsCancelled == true)) {
        libSCPL_gf_PlayerResourcesAdd(lv_playerIndex, Trunc(lv_tickTotalMinerals), Trunc(lv_tickTotalVespene));
    }
    if ((lv_progressIndex == 1)) {
