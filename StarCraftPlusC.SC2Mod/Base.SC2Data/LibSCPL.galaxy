        Wait(lv_tickIncrement, c_timeGame);
    }
    libSCPL_gf_ResourceDisplaySpendSubtract(lv_playerIndex, lv_tickCostBaseMinerals, lv_tickCostBaseVespene);
    if ((lv_progressIsCancelled == true)) {
