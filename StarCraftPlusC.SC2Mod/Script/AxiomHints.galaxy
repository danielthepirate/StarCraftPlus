void AxiomHints_ShowHint(int dlg, int player){
	DialogControlSetVisible (dlg, PlayerGroupSingle(player), true);
}

bool Axiom_AutoAddControlGroupHelpTip_Func(bool c, bool a) { 

	int player = 1;
	AxiomHints_ShowHint(Axiom_AutoAddControlGroupHelpTip_Dlg, player);
	TriggerDebugOutput(1, StringToText(TriggerGetFunction(EventTrigger())), AxiomHint_DebugOutput);
	return true; 
}
bool Axiom_MultiTypeControlGroupHelpTip_Func(bool c, bool a){
//
	int player = EventPlayer();
	unit u = EventUnitProgressUnit();
	string ut = UnitGetType(u);
	trigger t = EventTrigger();
	
	//;
	if(UnitTypeTestAttribute(ut, c_unitAttributeStructure) || UnitTypeTestFlag (ut, c_unitFlagWorker)){
		return false;
	}
	
	AxiomHints_ShowHint(Axiom_MultiTypeControlGroupHelpTip_Dlg, player);
	TriggerDebugOutput(1, StringToText(TriggerGetFunction(t)), AxiomHint_DebugOutput);
	return true;
}
