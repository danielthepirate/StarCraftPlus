bool AxiomHints_Func (bool testConds, bool runActions) {
	return true;
}
bool AxiomHints_Welcome_Func (bool testConds, bool runActions) {
	int player;
	int [c_maxPlayers] Dlg;
	for(player=0;player < c_maxPlayers;player+=1){
		if (IsActiveUser(player)) {
			if (PlayerRace(player)=="Terr"){
				Dlg[player] = Axiom_Terr_BuildWallHelpTip_Dlg; 
			}
			else if (PlayerRace(player)=="Prot"){
				Dlg[player] = Axiom_Prot_BuildPylonHelpTip_Dlg; 
			}
			else if (PlayerRace(player)=="Zerg"){
				Dlg[player] = Axiom_Zerg_BuildPoolHelpTip_Dlg; 	
			}
			DialogControlSetVisible (Dlg[player], PlayerGroupSingle(player), true);
		}
	}
	return true;
}


